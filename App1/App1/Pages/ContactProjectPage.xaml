<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App1.ContactProjectPage">
    <ContentPage.Content>
        <StackLayout Margin="30">
            <Label Text="Телефон" TextColor="black"/>
            <Frame BorderColor="black" CornerRadius="0" >
                <Entry x:Name="Phone" Margin="-23" HeightRequest="40" />
            </Frame>
            <Label Text="Телефон" TextColor="black"/>
            <Frame BorderColor="black" CornerRadius="0" >
                <Entry x:Name="Phone_2" Margin="-23" HeightRequest="40" />
            </Frame>
            <Label Text="E-mail" TextColor="black"/>
            <Frame BorderColor="black" CornerRadius="0" >
                <Entry x:Name="E_Mail" Margin="-23" HeightRequest="40" />
            </Frame>
            <Label Text="Адрес" TextColor="black"/>
            <Frame BorderColor="black" CornerRadius="0" >
                <Entry x:Name="Adress" Margin="-23" HeightRequest="40" />
            </Frame>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>